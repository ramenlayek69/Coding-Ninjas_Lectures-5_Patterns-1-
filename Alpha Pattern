import java.util.Scanner;
public class Solution {

public static void main(String[] args) {

    Scanner s = new Scanner(System.in);
         int n = s.nextInt();
         char ch = 'A';
    	 int i = 1;
         while (i <= n)
         {
             int j = 1;
             while (j <= i)
             {
                 System.out.print(ch);
                 j++;
             }
             System.out.println();
             i++;
             ch++;
         }

	}

}
